{
	"extends": [
		"plugin:vue/essential",
		"eslint:recommended",
		"@vue/eslint-config-prettier/skip-formatting"
	],
	"parser": "vue-eslint-parser",
	"parserOptions": {
		"parser": "@babel/eslint-parser"
	},
	"plugins": ["vue"],
	"rules": {
		"implicit-arrow-linebreak": "off"
	},
	"overrides": [
		{
			"files": ["*.cjs"],
			"env": {
				"node": true
			}
		}
	]
}
